{
  "name": "ready-prep",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test,migrations}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test,migrations}/**/*.ts\" --fix",
    "prettier": "prettier \"{src,apps,libs,test,migrations}/**/*.ts\" --write",
    "test": "npm run test:e2e && npm run test:unit",
    "test:unit": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:reset": "run-s 'typeorm schema:drop' migrations",
    "migrations": "npm run migrations:build && npm run -- typeorm migration:run",
    "migrations:build": "cd migrations && tsc *.ts",
    "migration:generate": "npm run -- typeorm migration:generate -n",
    "migration:revert": "npm run migrations:build && npm run -- typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.14",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.14",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.14",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.16",
    "@nestjs/typeorm": "^7.1.5",
    "@sendgrid/mail": "^7.4.2",
    "@types/analytics-node": "^3.1.5",
    "analytics-node": "^5.0.0",
    "argon2": "^0.27.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-pg-simple": "^6.2.1",
    "cron-parser": "^3.3.0",
    "crypto-random-string": "^3.3.1",
    "csurf": "^1.11.0",
    "csv-parse": "^4.15.3",
    "datauri": "^3.0.0",
    "ejs": "^3.1.6",
    "express-session": "^1.17.1",
    "formik": "^2.2.6",
    "graphlib": "^2.1.8",
    "helmet": "^4.4.1",
    "jose": "^2.0.4",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "memorystore": "^1.6.5",
    "nanoid": "^3.1.23",
    "node-gyp": "^7.1.2",
    "npm": "^7.6.3",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.1.70",
    "pg": "^8.5.1",
    "rebuild": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "stripe": "^8.138.0",
    "threads": "^1.6.3",
    "typeorm": "^0.2.32",
    "xlsx": "^0.16.9",
    "yup": "^0.32.9",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.14",
    "@types/connect-pg-simple": "^4.2.2",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.3",
    "@types/graphlib": "^2.1.7",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.35",
    "@types/nodemailer": "^6.4.1",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-local": "^1.0.33",
    "@types/sinon": "^9.0.11",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "import-sort-parser-typescript": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "sinon": "^9.2.4",
    "supertest": "^6.1.3",
    "ts-jest": "26.5.3",
    "ts-loader": "^8.0.18",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-etc": "^1.13.9",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
